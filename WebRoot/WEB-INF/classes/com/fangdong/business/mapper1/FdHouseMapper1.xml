  <select id="selectAllHouseVo" resultMap="VoResultMap">
  	select 
    h.id, h.owner_id, h.create_date, h.size, h.apartment_layout, h.remark, h.address, h.property_rights, 
    h.house_type, h.rent_price, h.region_id,u.username as 'owner'
    from fd_house h 
    left join fd_user u on h.owner_id=u.id
  </select>
  <select id="selectTopHouseVo" parameterType="map" resultMap="VoResultMap">
  	select 
    h.id, h.owner_id, h.create_date, h.size, h.apartment_layout, h.remark, h.address, h.property_rights, 
    h.house_type, h.rent_price, h.region_id,r1.region_name as 'area',r2.region_name as 'city',r3.region_name as 'province'
    from fd_house h 
    left join fd_region r1 on h.region_id = r1.id
    left join fd_region r2 on r1.parent_reg_id=r2.id
    left join fd_region r3 on r2.parent_reg_id=r3.id
    where r3.id=#{regionId}
    limit 0,#{top};
  </select>
