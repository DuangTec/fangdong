<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="fangdongTables" targetRuntime="MyBatis3" >

		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/fangdong" userId="root"
			password="hr12345@SQL">
		</jdbcConnection>
		
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.fangdong.business.model"
			targetProject=".\src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.fangdong.business.mapper"
			targetProject=".\src">
			<property name="enableSubPackages" value="true" />	
		</sqlMapGenerator>

 		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.fangdong.business.mapper" targetProject=".\src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		
		<table tableName="fd_hot_region"></table>
		<table tableName="fd_subway"></table>
		<table tableName="fd_subway-region"></table>
		

	</context>
</generatorConfiguration>